<mvc:View controllerName="financialhub.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">

    <Page id="page1" title="{i18n>title}"  class="sapMIBar.sapMHeader-CTX" >
        <content>
            <VBox class="customVBoxBackground">
                <List headerText="options" class="customListBackground">
                    <items>
                        <StandardListItem title="Expenditure Prediction" type="Active" press="onEP" icon="sap-icon://money-bills"/>
                        <StandardListItem title="loans" type="Active" press="onLoans" icon="sap-icon://loan"/>
                        <StandardListItem title="walletHistory" type="Active" press="onWH" icon="sap-icon://wallet"/>
                        <StandardListItem title="feedback" type="Active" press="onFed" icon="sap-icon://feedback"/>
                    </items>
                </List>
            </VBox>
        </content>
    </Page>
</mvc:View>
